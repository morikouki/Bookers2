<table class="table">
	<thead>
		<tr>
			<th>image</th>
			<th>name</th>
			<th colspan=4></th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
		<tr>
			<td><%= attachment_image_tag user, :profile_image, :fill, 80, 80, format: 'jpeg', class: "pull-left profile-img", fallback: "noimage.jpg",size:'100x100' %></td>
			<td><%= user.name %></td>
			<td>
				<%= link_to 'Show', user_path(user) %>
			</td>
		</tr>
		<% end %>
	</tbody>
</table>